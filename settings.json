{
	"editor.accessibilitySupport": "off",
	"editor.fontFamily": "BerkeleyMono Nerd Font, Monaco, 'Courier New', monospace",
	"editor.fontSize": 15,
	"window.zoomLevel": 0.2,
	"editor.inlineSuggest.enabled": true,
	"redhat.telemetry.enabled": false,
	"terminal.integrated.env.osx": {
		"FIG_NEW_SESSION": "1"
	},
	"totalTypeScript.hideAllTips": false,
	"totalTypeScript.hideBasicTips": false,
	"workbench.colorTheme": "Monokai Pro (Filter Spectrum)",
	"git.confirmSync": false,
	"[jsonc]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"vim.cursorStylePerMode.insert": "line",
	"vim.easymotion": true,
	"vim.incsearch": true,
	"vim.useSystemClipboard": true,
	"vim.useCtrlKeys": true,
	"vim.hlsearch": true,
	"vim.insertModeKeyBindings": [
		{
			"before": [
				"j",
				"j"
			],
			"after": [
				"<Esc>"
			]
		},
		{
			"before": [
				"j",
				"k"
			],
			"after": [
				"<Esc>"
			]
		}
	],
	"vim.leader": "<space>",
	"vim.handleKeys": {
		"<C-a>": false,
		"<C-f>": false
	},
	"vim.normalModeKeyBindingsNonRecursive": [
		{
			"before": [
				"<leader>",
				"s",
				"f"
			],
			"commands": [
				"workbench.action.quickOpen"
			]
		},
		{
			"before": [
				"<leader>",
				"b"
			],
			"commands": [
				"workbench.action.toggleSidebarVisibility"
			]
		},
		{
			"before": [
				"K"
			],
			"commands": [
				"editor.action.showHover"
			]
		},
		{
			"before": [
				"g",
				"d"
			],
			"commands": [
				"editor.action.goToDeclaration"
			]
		},
		{
			"before": [
				"<leader>",
				"r",
				"n"
			],
			"commands": [
				"editor.action.rename",
			]
		},
	],
	"vim.visualModeKeyBindingsNonRecursive": [
		{
			"before": [
				"<leader>",
				"r",
				"n"
			],
			"commands": [
				"editor.action.rename",
			]
		},
		{
			"before": [
				"J"
			],
			"commands": [
				"editor.action.moveLinesUpAction"
			],
			"when": "editorTextFocus && !editorReadonly"
		},
		{
			"before": [
				"K"
			],
			"commands": [
				"editor.action.moveLinesDownAction"
			],
			"when": "editorTextFocus && !editorReadonly"
		},
		{
			"before": [
				">"
			],
			"commands": [
				"editor.action.indentLines"
			]
		},
		{
			"before": [
				"<"
			],
			"commands": [
				"editor.action.outdentLines"
			]
		},
		{
			"before": [
				"p"
			],
			"after": [
				"p",
				"g",
				"v",
				"y"
			]
		}
	],
	"vim.statusBarColorControl": true,
	"vim.statusBarColors.normal": "#A1C281",
	"vim.statusBarColors.insert": "#74ADE9",
	"vim.statusBarColors.visual": "#BB7CD7",
	"vim.statusBarColors.visualline": "#BB7CD7",
	"vim.statusBarColors.visualblock": "#BB7CD7",
	"vim.statusBarColors.replace": "#D08770",
	"vim.statusBarColors.commandlineinprogress": "#ffffff",
	"vim.statusBarColors.searchinprogressmode": "#ffffff",
	"vim.statusBarColors.easymotionmode": "#007ACC",
	"vim.statusBarColors.easymotioninputmode": "#007ACC",
	"vim.statusBarColors.surroundinputmode": "#007ACC",
	"vim.searchHighlightColor": "rgba(250, 240, 170, 0.5)",
	"vim.highlightedyank.enable": true,
	"vim.highlightedyank.color": "rgba(250, 250, 250, 0.2)",
	"vim.substitutionColor": "rgba(250, 240, 170, 0.5)",
	"workbench.colorCustomizations": {
		"statusBar.background": "#BB7CD7",
		"statusBar.noFolderBackground": "#BB7CD7",
		"statusBar.debuggingBackground": "#BB7CD7",
		"statusBar.foreground": "#434C5E",
		"statusBar.debuggingForeground": "#434C5E",
		"statusBar.fontWeight": "bold",
		"editorInlayHint.background": "#00000000",
		"editorInlayHint.foreground": "#7f8c9a"
	},
	"totalTypeScript.hiddenTips": [
		"variable-type-annotation"
	],
	"javascript.inlayHints.enumMemberValues.enabled": true,
	"javascript.inlayHints.functionLikeReturnTypes.enabled": true,
	"javascript.inlayHints.parameterNames.enabled": "all",
	"javascript.inlayHints.propertyDeclarationTypes.enabled": true,
	"javascript.inlayHints.parameterTypes.enabled": true,
	"javascript.inlayHints.variableTypes.enabled": true,
	"typescript.inlayHints.enumMemberValues.enabled": true,
	"typescript.inlayHints.functionLikeReturnTypes.enabled": true,
	"typescript.inlayHints.parameterNames.enabled": "all",
	"typescript.inlayHints.parameterTypes.enabled": true,
	"typescript.inlayHints.propertyDeclarationTypes.enabled": true,
	"typescript.inlayHints.variableTypes.enabled": true,
	"editor.inlayHints.padding": true,
	"rust-analyzer.inlayHints.bindingModeHints.enable": true,
	"rust-analyzer.inlayHints.closureCaptureHints.enable": true,
	"rust-analyzer.inlayHints.closureReturnTypeHints.enable": "always",
	"rust-analyzer.inlayHints.discriminantHints.enable": "always",
	"rust-analyzer.inlayHints.expressionAdjustmentHints.enable": "always",
	"rust-analyzer.inlayHints.lifetimeElisionHints.useParameterNames": true,
	"rust-analyzer.inlayHints.typeHints.hideClosureInitialization": true,
	"rust-analyzer.inlayHints.typeHints.hideNamedConstructor": true,
	"rust-analyzer.inlayHints.reborrowHints.enable": "always",
	"rust-analyzer.inlayHints.closureStyle": "rust_analyzer",
	"[markdown]": {
		"editor.defaultFormatter": "yzhang.markdown-all-in-one"
	},
	"github.copilot.enable": {
		"*": true,
		"plaintext": false,
		"markdown": true,
		"scminput": false
	},
	"git.enableSmartCommit": true,
	"workbench.iconTheme": "material-icon-theme",
	"wallaby.startAutomatically": false,
	"terminal.external.osxExec": "Warp.app",
	"terminal.integrated.fontSize": 14,
	"editor.scrollbar.vertical": "hidden",
	"editor.scrollbar.horizontal": "hidden",
	"editor.lineNumbers": "relative",
	"editor.renderLineHighlight": "gutter",
	"files.watcherExclude": {
		"**/.bloop": true,
		"**/.metals": true,
		"**/.ammonite": true
	},
	"files.autoSave": "onFocusChange",
	"workbench.sideBar.location": "right"
}
